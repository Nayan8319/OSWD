<%- include('../partials/header') %>
<h1>Categories</h1>
<a href="/admin/categories/add">Add New Category</a>
<ul>
  <% categories.forEach(cat => { %>
    <li>
      <%= cat.name %> 
      <% if(cat.parent) { %> (Parent: <%= cat.parent.name %>) <% } %>
    </li>
  <% }) %>
</ul>
<%- include('../partials/footer') %>
